{
  "productName": "MultiDown",
  "version": "0.1.0",
  "identifier": "com.multidown.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build:all",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "MultiDown",
        "width": 900,
        "height": 600,
        "minWidth": 900,
        "minHeight": 480,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "resources": {
      "../dist-extension/unpacked": "extension/unpacked",
      "../dist-extension/multidown-extension.crx": "extension/multidown-extension.crx",
      "../integration/extension/com.multidown.app.json": "native-host/com.multidown.app.json",
      "../integration/native-host/target/release/multidown-native-host.exe": "native-host/multidown-native-host.exe"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "MultiDown - 对标 IDM 的跨平台多线程下载工具，支持动态分段与断点续传。",
    "shortDescription": "跨平台多线程下载工具",
    "macOS": {
      "minimumSystemVersion": "10.13"
    },
    "category": "Utility",
    "copyright": "",
    "license": "MIT"
  },
  "plugins": {}
}
