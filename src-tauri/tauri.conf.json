{
  "productName": "multidown",
  "version": "0.1.0",
  "identifier": "com.multidown.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Multidown",
        "width": 900,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "Multidown - 对标 IDM 的跨平台多线程下载工具，支持动态分段与断点续传。",
    "shortDescription": "跨平台多线程下载工具",
    "macOS": {
      "minimumSystemVersion": "10.13"
    },
    "category": "Utility",
    "copyright": "",
    "license": "MIT"
  },
  "plugins": {}
}
